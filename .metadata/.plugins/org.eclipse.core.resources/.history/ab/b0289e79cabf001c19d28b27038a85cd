public class Student {
	private int roll_no;
	   private String name;
	   private int age;

	   Student(int roll_no, String name, int age){
	      this.roll_no = roll_no;
	      this.name = name;
	      this.age = age;
	   }

	   public int getRollNo(){
	      return roll_no;
	   }
	   public int getAge(){
	      return age;
	   }
	   public String getName(){
	      return name;
	   }

	@Override
	public String toString() {
		return "roll_no=" + roll_no + ", name=" + name + ", age=" + age;
	}
	   


}



