package threadpool;

import java.util.Date;

public class Task implements Runnable{
public String name;
public Task(String name) {
	this.name=name;
}

	@Override
	public void run() {
		try {
			for(int i=0;i<5;i++) {
				if(i==0) {
					Date today=new Date();
					SimpleDateFormate ft=new SimpleDateFormate("hh:mm:ss");
				}
			}
		}catch(Exception e) {
			
		}
	}

}

