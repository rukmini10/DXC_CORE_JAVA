import java.util.Scanner;

public class Hospital {

}

class doctor{
	int doctor_id;
	String doctor_name;
	String doctor_specialist;
	String doctor_contact;
	String doctor_education;
	
	void addDoctor() {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Doctors Id: ");
		doctor_id=sc.nextInt();
		System.out.println("Enter Doctors Name: ");
		doctor_name=sc.nextLine();
		System.out.println("Enter Doctors Specialisation: ");
		doctor_specialist=sc.nextLine();
		System.out.println("Enter Doctors Contact Number: ");
		doctor_contact=sc.nextLine();
		System.out.println("Enter Doctors Education: ");
		doctor_education=sc.nextLine();
			
	}
	
	void displayDoctor() {
		return "doctor_id=" + doctor_id + ", doctor_name=" + doctor_name + ", doctor_specialist="
				+ doctor_specialist + ", doctor_contact=" + doctor_contact + ", doctor_education=" + doctor_education ;
	}
}